{
  "name": "generator-electron-react",
  "version": "1.0.0",
  "description": "A simple and compact boilerplate for electron and react (redux, router)",
  "scripts": {
	"hot": "webpack-dev-server --color --config ./configs/webpack.dev.config.js",
	"dev": "NODE_ENV=development electron ./app/src/main/app.js",
	"prod": "webpack -p --progress --color --config ./configs/webpack.prod.config.js && electron ./app/build/app.js"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
		"yeoman-generator",
		"Electron",
		"react"
  ],
  "author": "Tahnik Mustasin",
  "devDependencies": {
		"babel-core": "^6.10.4",
		"babel-loader": "^6.2.4",
		"babel-polyfill": "^6.23.0",
		"babel-preset-es2015": "^6.9.0",
		"babel-preset-react": "^6.11.1",
		"babel-preset-stage-1": "^6.5.0",
		"copy-webpack-plugin": "^4.0.1",
		"electron": "^1.4.1",
		"eslint": "^3.17.1",
		"eslint-config-google": "^0.7.1",
		"express": "^4.15.2",
		"react": "^15.2.1",
		"react-dom": "^15.2.1",
		"react-hot-loader": "^3.0.0-beta.6",
		"react-redux": "^5.0.3",
		"react-router": "4.0.0-beta.6",
		"react-router-dom": "4.0.0-beta.6",
		"redux": "^3.5.2",
		"webpack": "^2.2.1",
		"webpack-dev-server": "^2.4.1",
		"webpack-node-externals": "^1.2.0",
		"write-file-webpack-plugin": "^4.0.0"
  },
  "dependencies": {
		"yeoman-generator": "^1.1.1"
  }
}
